{
  "version": 2,
  "name": "biblio-frontend",
  "builds": [
    {
      "src": "frontend-gestion-biblio/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend-gestion-biblio/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend-gestion-biblio/dist/$1"
    },
    {
      "src": "/.*",
      "dest": "/frontend-gestion-biblio/dist/index.html"
    }
  ],
  "buildCommand": "cd frontend-gestion-biblio && npm run build",
  "outputDirectory": "frontend-gestion-biblio/dist",
  "installCommand": "cd frontend-gestion-biblio && npm install"
}
